{
  "name": "asp-js",
  "version": "0.1.0",
  "description": "",
  "main": "build/lib",
  "types": "build/lib",
  "scripts": {
    "test": "jest",
    "install": "tsc",
    "watch": "jest --watch",
    "example": "tsc && cd example && npm test"
  },
  "author": "Nils Ceberg",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "@types/koa": "^2.0.46",
    "@types/node": "^10.3.3",
    "jest": "^24.8.0",
    "source-map-support": "^0.5.6",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "parser-monad": "git+https://github.com/nilsceberg/parser-monad"
  }
}
